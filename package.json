{
  "name": "expense-platform",
  "version": "1.0.0",
  "description": "Enterprise-grade expense management platform - Airbase replacement",
  "main": "dist/src/server.js",
  "scripts": {
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register src/server.ts",
    "dev": "nodemon",
    "build": "echo 'Skipping tsc build (transpile at runtime)'",
    "typecheck": "tsc --noEmit",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "integration-test": "node start-integration-test.js",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "node -r ts-node/register/transpile-only -r tsconfig-paths/register src/scripts/migrate.ts",
    "seed": "node -r ts-node/register/transpile-only -r tsconfig-paths/register src/scripts/seed.ts",
    "health-check": "node -r ts-node/register/transpile-only -r tsconfig-paths/register src/scripts/health-check.ts",
    "plaid:sync": "node dist/src/scripts/sync-plaid.js",
    "plaid:cleanup": "node dist/src/scripts/cleanup-plaid.js"
  },
  "keywords": [
    "expense",
    "platform",
    "enterprise",
    "airbase",
    "plaid",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.668.0",
    "@aws-sdk/client-ses": "^3.668.0",
    "@aws-sdk/client-textract": "^3.668.0",
    "@aws-sdk/s3-request-presigner": "^3.668.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^17.2.2",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "knex": "^3.0.1",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-saml": "^3.2.4",
    "pg": "^8.11.1",
    "plaid": "^15.0.0",
    "qrcode": "^1.5.3",
    "redis": "^4.6.7",
    "sharp": "^0.33.5",
    "speakeasy": "^2.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.9.2",
    "uuid": "^9.0.1",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/csurf": "^1.11.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mime-types": "^3.0.1",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.3.3",
    "@types/nodemailer": "^6.4.8",
    "@types/passport": "^1.0.12",
    "@types/pg": "^8.10.2",
    "@types/qrcode": "^1.5.1",
    "@types/speakeasy": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^6.0.1",
